package Pessoa;

class Animal {

	String Animal;
    String cor;
    int Idade;
	String raça;

    public void setAnimal(String Animal) {
        this.Animal = Animal;
    }

    public String getAnimal() {
        return Animal;
    }

    public void setCor(String cor) {
        this.cor = cor;
    }

    public String getCor() {
        return cor;
    }

    public void setIdade(int idade) {
        this.Idade = idade;
    }

    public int getIdade() {
        return Idade;
    }

    public void setraça(String raça) {
        this.raça = raça;
    }

    public String getraça() {
        return raça;
    }

    
    public String miar() {
        return "Miando";
    }

    public String dormir() {
    	return "Dormindo";
    }

    public String comer() {
    	return "Comendo";
    }
    public String latir() {
    	return "Latindo";
	}
    public String mugir() {
    	return "Mugindo";
	}
    public String cantar() {
    	return "Cantando";
	}
    public String berrar() {
    	return "Berrando";
	}
    public String piar() {
    	return "Piando";
	}
    public String pular() {
    	return "Pulando";
}
}
